<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroud"
    tools:context="com.infinitus.yearapp_a.MainActivity" >

    <!--<ImageView-->
        <!--android:id="@+id/fl_logo"-->
        <!--android:layout_width="@dimen/x259"-->
        <!--android:layout_height="@dimen/y261"-->
        <!--android:layout_centerHorizontal="true"-->
        <!--android:layout_marginTop="@dimen/y35"-->
        <!--android:background="@drawable/ic_logo_bg_small"-->
        <!--android:scaleType="center"-->
        <!--android:src="@drawable/ic_logo_small" />-->

    <RelativeLayout
        android:id="@+id/rl_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/y35"
        >
        <ImageView
            android:id="@+id/iv_logo_background"
            android:layout_width="@dimen/x326"
            android:layout_height="@dimen/y330"
            android:layout_centerInParent="true"
            android:scaleType="fitXY"
            android:src="@drawable/ic_logo_bg_large"/>
        <ImageView
            android:layout_width="@dimen/x208"
            android:layout_height="@dimen/y213"
            android:layout_centerInParent="true"
            android:scaleType="fitXY"
            android:src="@drawable/ic_logo_large"/>
    </RelativeLayout>

    <ImageView
        android:id="@+id/iv_kouhao"
        android:layout_width="@dimen/x477"
        android:layout_height="@dimen/y116"
        android:layout_below="@id/rl_logo"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/y25"
        android:src="@drawable/kouhao_logo" />

    <LinearLayout
        android:id="@+id/ll_menu_line1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y150"
        android:layout_marginTop="@dimen/y35"
        android:layout_marginLeft="@dimen/x30"
        android:layout_marginRight="@dimen/x30"
        android:layout_below="@id/iv_kouhao" >

        <LinearLayout
            android:id="@+id/ll_schedule"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="@dimen/y150"
            android:background="@drawable/bg_main_menu_item"
            android:gravity="center" >

            <ImageView
                android:layout_width="@dimen/x76"
                android:layout_height="@dimen/y97"
                android:src="@drawable/icon_schedule" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_gonglue"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="@dimen/y150"
            android:layout_marginLeft="@dimen/x10"
            android:background="@drawable/bg_main_menu_item"
            android:gravity="center" >

            <ImageView
                android:layout_width="@dimen/x76"
                android:layout_height="@dimen/y97"
                android:src="@drawable/icon_gonglue" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_menu_line2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y150"
         android:layout_marginTop="@dimen/y10"
        android:layout_marginLeft="@dimen/x30"
        android:layout_marginRight="@dimen/x30"
        android:layout_below="@id/ll_menu_line1" >

        <LinearLayout
            android:id="@+id/ll_xiangji"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="@dimen/y150"
            android:background="@drawable/bg_main_menu_item"
            android:gravity="center" >

            <ImageView
                android:layout_width="@dimen/x76"
                android:layout_height="@dimen/y97"
                android:src="@drawable/icon_xiangji" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_3d"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="@dimen/y150"
            android:layout_marginLeft="@dimen/x10"
            android:background="@drawable/bg_main_menu_item"
            android:gravity="center" >

            <ImageView
                android:layout_width="@dimen/x76"
                android:layout_height="@dimen/y97"
                android:src="@drawable/icon_3d" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y150"
         android:layout_marginTop="@dimen/y10"
        android:layout_marginLeft="@dimen/x30"
        android:layout_marginRight="@dimen/x30"
        android:layout_below="@id/ll_menu_line2" >

        <LinearLayout
            android:id="@+id/ll_flashlight"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="@dimen/y150"
            android:background="@drawable/bg_main_menu_item"
            android:gravity="center">

            <ImageView
                android:layout_width="@dimen/x76"
                android:layout_height="@dimen/y97"
                android:src="@drawable/icon_flashlight" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_xiangce"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="@dimen/y150"
            android:layout_marginLeft="@dimen/x10"
            android:background="@drawable/bg_main_menu_item"
            android:gravity="center">

            <ImageView
                android:layout_width="@dimen/x76"
                android:layout_height="@dimen/y97"
                android:src="@drawable/icon_xiangce" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_scan"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="@dimen/y150"
            android:layout_marginLeft="@dimen/x10"
            android:background="@drawable/bg_main_menu_item"
            android:gravity="center" >

            <ImageView
                android:layout_width="@dimen/x76"
                android:layout_height="@dimen/y97"
                android:src="@drawable/icon_scan" />
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>